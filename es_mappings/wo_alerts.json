{
    "template": "alerts_wo*",
    "settings": {
        "index": {
            "number_of_shards": 1,
            "number_of_replicas": 0,
            "percolator": {
                "map_unmapped_fields_as_string": true
            }
        },
        "analysis": {
            "analyzer": {
                "default": {
                    "type": "dutch"
                },
                "text_nl": {
                    "type": "custom",
                    "filter": [
                        "asciifolding",
                        "lowercase",
                        "custom_word_delimiter"
                    ],
                    "tokenizer": "standard"
                }
            },
            "filter": {
                "custom_word_delimiter": {
                    "type": "word_delimiter",
                    "split_on_numerics": false,
                    "generate_number_parts": true,
                    "preserve_original": true,
                    "stem_english_possesive": false,
                    "catenate_words": false,
                    "catenate_numbers": true,
                    "split_on_case_change": false,
                    "generate_word_parts": true
                }
            }
        }
    },
    "mappings": {
        "_default_": {
            "_all": {
                "enabled": false
            },
            "properties": {
                "email": {"type": "keyword"},
                "token": {"type": "keyword"},
                "querystring": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "activated": {"type": "boolean"},
                "query": {"type": "percolator"}
            }
        },
        "document": {
            "properties": {
                "meta": {
                    "type": "object",
                    "properties": {
                        "source_id": {
                            "type": "keyword"
                        },
                        "collection": {
                            "type": "keyword"
                        },
                        "original_object_id": {
                            "type": "keyword"
                        },
                        "rights": {
                            "type": "keyword"
                        },
                        "original_object_urls": {
                            "type": "object"
                        },
                        "processing_started": {
                            "type": "date",
                            "format": "dateOptionalTime"
                        },
                        "processing_finished": {
                            "type": "date",
                            "format": "dateOptionalTime"
                        }
                    }
                },
                "all_text": {
                    "type": "text",
                    "analyzer": "text_nl"
                },
                "title": {
                    "type": "text",
                    "analyzer": "text_nl"
                },
                "name": {
                    "type": "text",
                    "fields" : {
                        "keyword" : {
                            "type" : "keyword",
                            "ignore_above" : 256
                        }
                    }
                },
                "description": {
                    "type": "text",
                    "analyzer": "text_nl"
                },
                "classification": {
                    "type": "keyword"
                },
                "date": {
                    "type": "date",
                    "format": "dateOptionalTime"
                },
                "start_date": {
                    "type": "date"
                },
                "end_date": {
                    "type": "date"
                },
                "date_granularity": {
                    "type": "short"
                },
                "organization" : {
                    "properties" : {
                        "description" : {
                            "type" : "text",
                            "fields" : {
                                "keyword" : {
                                    "type" : "keyword",
                                    "ignore_above" : 256
                                }
                            }
                        },
                        "id" : {
                            "type" : "text",
                            "fields" : {
                                "keyword" : {
                                    "type" : "keyword",
                                    "ignore_above" : 256
                                }
                            }
                        },
                        "name" : {
                            "type" : "text",
                            "fields" : {
                                "keyword" : {
                                    "type" : "keyword",
                                    "ignore_above" : 256
                                }
                            }
                        }
                    }
                },
                "authors": {
                    "type": "text",
                    "fields": {
                        "authors": {
                            "type" : "text",
                            "analyzer": "text_nl"
                        },
                        "untouched": {
                            "type": "keyword"
                        }
                    }
                },
                "media_urls": {
                    "properties": {
                        "url": {
                            "type": "keyword",
                            "index": "false",
                            "copy_to": "media_url"
                        },
                        "original_url": {
                            "type": "keyword",
                            "index": "false",
                            "copy_to": "media_url"
                        },
                        "width": {
                            "type": "integer",
                            "copy_to": "media_url"
                        },
                        "height": {
                            "type": "integer",
                            "copy_to": "media_url"
                        },
                        "content_type": {
                            "type": "keyword",
                            "copy_to": "media_url"
                        }
                    }
                },
                "enrichments": {
                    "type": "object",
                    "properties": {
                        "media_urls": {
                            "properties": {
                                "url": {
                                    "type": "keyword",
                                    "index": "false"
                                },
                                "original_url": {
                                    "type": "keyword",
                                    "index": "false"
                                },
                                "media_type": {
                                    "type": "keyword"
                                },
                                "content_type": {
                                    "type": "keyword"
                                },
                                "size_in_bytes": {
                                    "type": "integer"
                                },
                                "resolution": {
                                    "type": "object",
                                    "properties": {
                                        "width": {
                                            "type": "integer"
                                        },
                                        "height": {
                                            "type": "integer"
                                        },
                                        "total_pixels": {
                                            "type": "integer"
                                        }
                                    }
                                },
                                "image_format": {
                                    "type": "keyword"
                                },
                                "image_mode": {
                                    "type": "keyword"
                                }
                            }
                        }
                    }
                },
                "source_data": {
                    "type": "object",
                    "properties": {
                        "data": {
                            "type": "text",
                            "index": "false",
                            "doc_values": "false"
                        },
                        "content_type": {
                            "type": "keyword"
                        }
                    }
                },
                "sources" : {
                    "properties" : {
                        "description" : {
                            "type": "text",
                            "analyzer": "text_nl"
                        },
                        "note" : {
                            "type": "text",
                            "analyzer": "text_nl"
                        },
                        "url" : {
                            "type": "keyword",
                            "index": "false"
                        },
                        "annotations": {
                            "type": "object",
                            "enabled": "false"
                        },
                        "snippets": {
                            "type": "text",
                            "analyzer": "text_nl"
                        },
                        "snippets_by_code": {
                            "type": "object",
                            "enabled": "false"
                        }
                    }
                },

                "combined_index_data": {
                    "type": "text",
                    "index": "false",
                    "doc_values": "false"
                },
                "districts": {
                    "type": "keyword"
                },
                "neighborhoods": {
                    "type": "keyword"
                },
                "annotations": {
                    "type": "object",
                    "enabled": "false"
                },
                "snippets": {
                    "type": "text",
                    "analyzer": "text_nl"
                },
                "snippets_by_code": {
                    "type": "object",
                    "enabled": "false"
                }
            }
        }
    }
}
